{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.3.min.js", "content.js"]
    }
  ],
  "browser_action": {
  "default_icon": "icon.png"

},

"background": {
  "scripts": ["background.js"]
},

"commands": {
  "toggle-feature": {
    "suggested_key": {
      "default": "Ctrl+Shift+U",
      "windows": "Ctrl+Shift+U",
      "mac": "Command+Shift+U",
      "chromeos": "Ctrl+Shift+U",
      "linux": "Ctrl+Shift+U"
    },"description":"downloads"
  },
  "save-feature":{
    "suggested_key": {
      "default": "Ctrl+Shift+Z",
      "windows": "Ctrl+Shift+Z",
      "mac": "Command+Shift+Z",
      "chromeos": "Ctrl+Shift+Z",
      "linux": "Ctrl+Shift+Z"
    },"description":"adds to list of data"


  }
}

}
